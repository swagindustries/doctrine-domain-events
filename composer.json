{
    "name": "swag-industries/doctrine-domain-events",
    "type": "symfony-bundle",
    "license": "MIT",
    "require": {
        "php": ">=7.4",
        "symfony/event-dispatcher": "^5.0|^6.0",
        "doctrine/doctrine-bundle": "^1.8|^2.0",
        "doctrine/orm": "^2.6.3"
    },
    "authors": [
        {
            "name": "Maxime Veber",
            "homepage": "https://www.nekland.fr"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "friendsofphp/php-cs-fixer": "^2.14",
        "symfony/symfony": "^5.0|^6.0",
        "phpspec/prophecy": "^1.15"
    },
    "conflict": {
        "doctrine/orm": "<2.6.3"
    },
    "autoload": {
        "psr-4": {
            "Biig\\Component\\Domain\\": "./src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Biig\\Component\\Domain\\Tests\\": "./Tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    }
}
